<!DOCTYPE html>
<html>
<head>
  <title>
    <% if content_for?(:title) %>
      <%= yield(:title) %>
    <% else %>
      Ticketee
    <% end %>
  </title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<table cellpadding = 10 cellspacing = 10 align = right border-radious = 25px>
  <tr>
    <nav>
      <td align = center bgcolor= "sky blue"> <%= link_to "Sign up", new_user_path %>
      <td align = center bgcolor= "skyblue"><%= link_to "Sign in", signin_path %>
    </nav>
  </tr>
</table>

<% flash.each do |key,value| %>
  <div class='flash' id = '<%= key %>'>
    <%= value %>
  </div>
<% end %>
<%= yield %>

</body>
</html>
